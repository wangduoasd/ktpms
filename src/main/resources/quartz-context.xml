<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="scheduler" lazy-init="true" autowire="no" class="org.springframework.scheduling.quartz.SchedulerFactoryBean">

    </bean>
    <bean id="quartzManager" class="com.kaituo.pms.quartz.QuartzManager" lazy-init="false" init-method="startJobs">
        <property name="scheduler" ref="scheduler"></property>

    </bean>

</beans>